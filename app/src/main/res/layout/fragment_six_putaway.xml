<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_one_fragment"
    android:orientation="horizontal"
    android:paddingStart="@dimen/dimen_60"
    android:paddingEnd="@dimen/dimen_60"
    tools:context=".view.fragments.SixPutawayFragment">

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_dataAny"
        style="@style/medium_18sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_rounded_up_gray"
        android:gravity="center"
        android:singleLine="true"
        app:layout_constraintHeight_percent="0.15"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_scanData"
        style="@style/medium_18sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_grey_background"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_dataAny"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_desc1"
        style="@style/medium_18sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_grey_background"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_scanData"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_desc3"
        style="@style/medium_18sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_rounder_below_grey"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/txt_wearehouse_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_desc1"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_desc2"
        style="@style/medium_18sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/dimen_5_dp"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/txt_wearehouse_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_desc3"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomEditText
        android:id="@+id/edit_lic"
        style="@style/regular_16sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/dimen_80"
        android:layout_marginEnd="@dimen/dimen_80"
        android:layout_marginBottom="@dimen/dimen_10_dp"
        android:background="@drawable/et_rounded"
        android:gravity="center"
        android:imeOptions="flagNoExtractUi|actionDone"
        android:singleLine="true"
        android:maxLength="15"
        android:inputType="text"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_desc2" >
        <requestFocus />
    </com.bfc.putaway.customviews.PutAwayCustomEditText>

</androidx.constraintlayout.widget.ConstraintLayout>