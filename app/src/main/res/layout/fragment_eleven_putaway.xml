            android:background="@drawable/et_rounded"
            android:gravity="center"
            android:padding="@dimen/dimen_5_dp"
            android:inputType="text"
            android:maxLength="12"
            android:imeOptions="flagNoExtractUi|actionNext"
            android:singleLine="true"
            android:textColor="@color/colorBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent=".20"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_desc5"
            tools:text="Scan" />

        <com.bfc.putaway.customviews.PutAwayCustomEditText
            android:id="@+id/tv_desc7"
            style="@style/medium_16sp"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/dimen_10_dp"
            android:inputType="text"
            android:maxLength="12"
            android:background="@drawable/et_rounded"
            android:gravity="center"
            android:padding="@dimen/dimen_5_dp"
            android:imeOptions="flagNoExtractUi|actionNext"
            android:singleLine="true"
            android:textColor="@color/colorBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent=".20"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_desc6"
            tools:text="Scan" />

        <com.bfc.putaway.customviews.PutAwayCustomEditText
            android:id="@+id/tv_desc8"
            style="@style/medium_16sp"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/dimen_10_dp"
            android:inputType="text"
            android:maxLength="12"
            android:imeOptions="flagNoExtractUi|actionNext"
            android:background="@drawable/et_rounded"
            android:gravity="center"
            android:padding="@dimen/dimen_5_dp"
            android:singleLine="true"
            android:textColor="@color/colorBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent=".20"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_desc7"
            tools:text="Scan" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/endLayout"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:paddingStart="@dimen/dimen_5_dp"
        android:paddingEnd="@dimen/dimen_5_dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.30">

        <com.bfc.putaway.customviews.PutAwayCustomEditText
            android:id="@+id/tv_desc9"
