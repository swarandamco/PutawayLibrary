            tools:text="Scan" />

        <com.bfc.putaway.customviews.PutAwayCustomEditText
            android:id="@+id/tv_desc11"
            style="@style/medium_16sp"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/dimen_10_dp"
            android:background="@drawable/et_rounded"
            android:gravity="center"
            android:padding="@dimen/dimen_5_dp"
            android:imeOptions="flagNoExtractUi|actionNext"
            android:inputType="number"
            android:maxLength="5"
            android:singleLine="true"
            android:textColor="@color/colorBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent=".20"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_desc10"
            tools:text="Scan" />

        <com.bfc.putaway.customviews.PutAwayCustomEditText
            android:id="@+id/tv_desc12"
            android:layout_width="match_parent"
            style="@style/medium_16sp"
            android:layout_height="0dp"
            android:inputType="number"
            android:maxLength="5"
            android:imeOptions="flagNoExtractUi|actionNext"
            android:layout_marginTop="@dimen/dimen_10_dp"
            android:background="@drawable/et_rounded"
            android:gravity="center"
            android:padding="@dimen/dimen_5_dp"
            android:singleLine="true"
            android:textColor="@color/colorBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent=".20"
            app:layout_constraintStart_toStartOf="parent"
