<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingStart="@dimen/dimen_60"
    android:paddingEnd="@dimen/dimen_60"
    tools:context=".view.fragments.NinePutawayFragment">

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_dept"
        style="@style/medium_16sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/dimen_5_dp"
        android:background="@drawable/et_rounded_up_gray"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_fr"
        style="@style/medium_16sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_grey_background"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_dept"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_to"
        style="@style/medium_16sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_grey_background"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_fr"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_dataAny"
        style="@style/medium_16sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_grey_background"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_to"
        tools:text="Scan" />

    <com.bfc.putaway.customviews.PutAwayCustomTextView
        android:id="@+id/tv_qty"
        style="@style/medium_16sp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/et_rounder_below_grey"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@color/colorBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_dataAny"
        tools:text="Scan" />

</androidx.constraintlayout.widget.ConstraintLayout>